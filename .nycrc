{
  "verbose": false,
  "tempDirectory": "./coverage/.tmp",
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "watermarks": {
    "statements": [
      70,
      90
    ],
    "lines": [
      70,
      90
    ],
    "functions": [
      70,
      90
    ],
    "branches": [
      70,
      90
    ]
  }
}
